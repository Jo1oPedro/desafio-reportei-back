{
    "openapi": "3.0.0",
    "info": {
        "title": "API GithubAnalyzer Swagger Documentation",
        "contact": {
            "email": "joao.pedreira@estudante.ufjf.br"
        },
        "version": "1.0"
    },
    "paths": {
        "/api/register": {
            "post": {
                "tags": [
                    "User Register"
                ],
                "summary": "Register a new user",
                "description": "This endpoint register a new user and returns his autentication code",
                "operationId": "register",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "name",
                                    "github_login",
                                    "github_id",
                                    "avatar_url",
                                    "access_token"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "gabriel"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "gabriel_nunes@example.org"
                                    },
                                    "github_login": {
                                        "type": "string",
                                        "example": "#sdasd$ssdaAA@"
                                    },
                                    "github_id": {
                                        "type": "string",
                                        "example": "#sdasd$ssdaAA@"
                                    },
                                    "avatar_url": {
                                        "type": "string",
                                        "example": "#sdasd$ssdaAA@"
                                    },
                                    "access_token": {
                                        "description": "Token got from the github api",
                                        "type": "string",
                                        "example": "#sdasd$ssdaAA@"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Token generated",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "token": {
                                            "description": "The type of token",
                                            "type": "string",
                                            "example": "112124alsglasg"
                                        },
                                        "user": {
                                            "properties": {
                                                "github_id": {
                                                    "type": "string",
                                                    "example": "#sdasd$ssdaAA@"
                                                },
                                                "name": {
                                                    "type": "string",
                                                    "example": "gabriel"
                                                },
                                                "email": {
                                                    "type": "string",
                                                    "example": "gabriel_nunes@example.org"
                                                },
                                                "avatar_url": {
                                                    "type": "string",
                                                    "example": "#sdasd$ssdaAA@"
                                                },
                                                "github_login": {
                                                    "type": "string",
                                                    "example": "#sdasd$ssdaAA@"
                                                },
                                                "access_token": {
                                                    "type": "string",
                                                    "example": "#sdasd$ssdaAA@"
                                                },
                                                "updated_at": {
                                                    "type": "string",
                                                    "example": "2024-07-08T21:53:43.000000Z"
                                                },
                                                "created_at": {
                                                    "type": "string",
                                                    "example": "2024-07-08T21:53:43.000000Z"
                                                },
                                                "id": {
                                                    "type": "integer",
                                                    "example": 1
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Incorrect credentials",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "The provided credentials are incorrect."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "description": "Acess token obtido na autenticação",
                "name": "Authorization",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
        {
            "name": "User Register",
            "description": "User Register"
        }
    ]
}